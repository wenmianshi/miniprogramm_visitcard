                <image src="http://s59avscau.hd-bkt.clouddn.com/com3.jpg" mode="widthFix" class="com"></image>
            </swiper-item>
            <swiper-item>
                <image src="http://s59avscau.hd-bkt.clouddn.com/com4.jpg" mode="widthFix" class="com"></image>
            </swiper-item>
            <swiper-item>
                <image src="http://s59avscau.hd-bkt.clouddn.com/com5.jpg" mode="widthFix" class="com"></image>
